<a data-route="acception/{{blog.id}}/{{this.id}}">
  <span class="item-asset-container">
  {{#if this.assets.length}}
    {{#eachOnly this.assets 0}}
      <span class="item-asset" style="background-image:url({{url}})" /></span>
    {{/eachOnly}}
  {{else}}
      <span class="item-asset no-image"></span>
  {{/if}}
  </span>
  <span class="item-title">{{title}}</span>
  <span class="item-arrow icon-arrow-right"></span>
</a>
