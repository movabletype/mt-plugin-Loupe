<section id="stats-post" class="card">
  <div class="content">
  {{>js_views_card_partials_loading}}
  {{>js_views_card_partials_fetchError}}
  {{#if id}}
    {{#ifIs status eq="Publish"}}
      <div class="pageviews">
        <span class="label"><span class="label-inner">{{#trans trans}}{{label}}{{/trans}}</span></span>
        <span class="pageview">{{pageviews}}</span>
      </div>
      {{>js_views_card_partials_entryItemsList}}
    {{/ifIs}}
  {{/if}}
  </div>
</section>
