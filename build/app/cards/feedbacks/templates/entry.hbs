<header>
<h1>{{#trans trans}}The Post Commented{{/trans}}</h1>
</header>
<div class="content">
  {{>js_views_card_partials_fetchError}}
  {{>js_views_card_partials_loading}}
  {{#if id}}
  {{#ifIs status eq="Publish"}}
    {{>js_views_card_partials_entryItemsList}}
  {{else}}
  <div class="error overlay">
    <div class="overlay-inner">
      <span class="message overlay-content">
        <span class="message-inner overlay-content-inner">{{#trans trans}}The post is not published{{/trans}}</span>
      </span>
    </div>
  </div>
  {{/ifIs}}
  {{/if}}
</div>
