<header>
<h1 id="{{name}}-title">{{#trans trans}}{{{title}}}{{/trans}}</h1>
</header>
<div class="content">
  {{>js_views_card_partials_loading}}
  {{>js_views_card_partials_fetchError}}
  {{>js_views_card_partials_providerIsNotAvailable}}
  {{>js_views_card_partials_noItem}}
  <ul id="top-articles-list-{{unit}}" class="card-item-list top-articles-list">
  {{#each items}}
    <li id="eid-{{this.entry.id}}" data-pageviews="{{this.pageviews}}">
      <div class="list-item-inner">
        <div class="content-loading"></div>
        {{>cards_stats_templates_itemviewContent}}
      </div>
    </li>
  {{/each}}
  </ul>
  {{#if itemLoading}}
  <div class="item-loading"></div>
  {{/if}}
</div>
