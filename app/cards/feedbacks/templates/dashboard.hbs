<header>
<h1 id="feedbacks-label">{{#trans trans}}{{name}}{{/trans}}</h1>
<p id="feedbacks-count" class="card-header-count">{{totalResults}}</p>
</header>

{{#if loading}}
<div class="loading"></div>
{{/if}}
{{#if moreLoading}}
<div class="loading"></div>
{{/if}}
{{#if error}}
<div class="error">
  <div class="message">{{#trans trans}}Failed fetching data{{/trans}}</div>
  <div class="refetch">{{#trans trans}}re-fetch data{{/trans}}</div>
</div>
{{/if}}
<ul id="feedback-comments-list">
</ul>
<div id="feedbacks-readmore">
  さらに見る
</div>
